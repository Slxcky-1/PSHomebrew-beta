# Optional Environment Variables
# Copy this file to .env and configure as needed

# Encryption key for config.json encryption (optional)
CONFIG_ENCRYPTION_KEY=your_encryption_key_here

# Timezone for scheduled restart (default: UTC)
# Examples: America/New_York, Europe/London, Asia/Tokyo
RESTART_TIMEZONE=UTC

# Port for SellHub webhook server (default: 3000)
WEBHOOK_PORT=3000

# Node.js memory limit for low-end systems (optional)
# Uncomment and adjust if needed
# NODE_OPTIONS=--max-old-space-size=512
